{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-GB": {
          "summary": "Informs residents of Stroud District Council when the next bin day is and what waste collection type it is.",
          "keywords": [
            "bin collection day",
            "recycling",
            "stroud district council"
          ],
          "examplePhrases": [
            "Alexa, ask Stroud bins, when is bin day",
            "Alexa, ask Stroud bins, what bins need to go out",
			"Alexa, ask Stroud bins, what bins need to go out this week",
            "help"
          ],
          "name": "Bin Day: Stroud District Council",
          "description": "Informs residents of Stroud District Council when the next bin day is and what waste collection type it is."
        }
      },
      "isAvailableWorldwide": false,
      "testingInstructions": "Sample Testing Instructions.",
      "category": "KNOWLEDGE_AND_TRIVIA",
      "distributionMode": "PUBLIC"
    },
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "usesPersonalInfo": true,
      "isChildDirected": false,
      "isExportCompliant": false,
      "containsAds": false,
      "locales": {
        "en-GB": {
          "privacyPolicyUrl": "https://github.com/jgcrunden/stroud-bin-day/blob/master/PRIVACY-POLICY.md",
          "termsOfUseUrl": "https://github.com/jgcrunden/stroud-bin-day/blob/master/TERMS-OF-USE.md"
        }
      }
    },
    "permissions": [
      {
        "name": "alexa:devices:all:address:country_and_postal_code:read"
      }
    ],
    "apis": {
      "custom": {
        "interfaces": [
          {
            "type": "CAN_FULFILL_INTENT_REQUEST"
          }
        ],
        "endpoint": {
          "uri": "LAMBDA_ARN"
        }
      }
    },
    "manifestVersion": "1.0"
  }
}
